{
    "name": "webfactory/hotfolder-bundle",
    "description": "Symfony console command to upload files from a folder.",
    "license": "proprietary",
    "require": {
        "php": "8.1.*",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "exclude-from-classmap": [
            "src/bundles.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
